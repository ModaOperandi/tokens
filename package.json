{
  "name": "moda-themes",
  "version": "0.0.0-development",
  "description": "Constant themed values for modaoperandi.com",
  "repository": "http://github.com/ModaOperandi/moda-themes",
  "author": "Team Moda",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "style": "lib/assets/stylesheets/_moda-themes.scss",
  "files": [
    "./index.js",
    "/lib/assets",
    "/data"
  ],
  "scripts": {
    "build": "node scripts/export",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy:local": "gh-pages -d example/build",
    "deploy": "gh-pages --repo https://$GH_TOKEN@github.com/ModaOperandi/moda-themes.git -d example/build --silent",
    "start": "cd example && yarn start"
  },
  "devDependencies": {
    "gh-pages": "2.0.1",
    "node-sass": "^4.12.0",
    "node-sass-export": "^0.0.3"
  },
  "dependencies": {}
}
